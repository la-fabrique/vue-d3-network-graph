{
  "name": "vue-d3-network-graph",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/vue-d3-network-graph.umd.cjs",
  "module": "./dist/vue-d3-network-graph.js",
  "exports": {
    ".": {
      "import": "./dist/vue-d3-network-graph.js",
      "require": "./dist/vue-d3-network-graph.umd.cjs"
    }
  },
  "types": "./dist/vue-d3-network-graph.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write"
  },
  "dependencies": {
    "@vueuse/core": "^7.7.1",
    "d3": "^7.3.0",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@mdi/font": "5.9.55",
    "@mdi/js": "^7.1.96",
    "@types/d3": "^7.4.0",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.8.0",
    "lint-staged": "^12.5.0",
    "prettier": "^2.8.1",
    "roboto-fontface": "0.10.0",
    "sass": "^1.57.1",
    "sass-loader": "^10.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vuetify": "^1.0.1",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.22",
    "vuetify": "^3.0.0-beta.15",
    "vuetify-loader": "^2.0.0-alpha.9",
    "webfontloader": "^1.6.28"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:vue/vue3-recommended",
      "prettier"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "vue/no-v-html": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
